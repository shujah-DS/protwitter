{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "window": true,
    "document": true,
    "navigator": true,
    "faker": true
  },
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "global-require": 0,
    "import/prefer-default-export": 0,
    "react/prop-types": 0,
    "react/destructuring-assignment": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "linebreak-style": 0,
    "arrow-parens": 0,
    "object-curly-newline": 0,
    "implicit-arrow-linebreak": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "max-len": 0,
    "react/jsx-no-bind": 0,
    "no-param-reassign": 0,
    "react/no-this-in-sfc": 0
  }
}
